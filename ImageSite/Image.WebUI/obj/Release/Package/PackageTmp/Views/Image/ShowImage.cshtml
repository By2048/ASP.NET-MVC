@model IEnumerable<Image.EF.Concrete.Images>

@{
    ViewBag.Title = "ShowImage";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<style>
    .img {
        width: 100px;
    }
</style>

<ul>
    <li>
        @foreach (var item in Model)
        {
            <img src="@item.Path" alt="@item.Name" class="img-thumbnail" />
        }
    </li>
</ul>
